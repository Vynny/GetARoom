package com.soen343.session;

public class ReservationSession {

	private long userId;
	private long roomId;
	
	public ReservationSession(long userId, long roomId) {
		this.userId = userId;
		this.setRoomId(roomId);
	}

	public long getRoomId() {
		return roomId;
	}

	public void setRoomId(long roomId) {
		this.roomId = roomId;
	}
	
	
	
	
}
